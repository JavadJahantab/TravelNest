 <div class="col-lg-6">
    <div class="contact-form">
      <form  method="post">
        <div class="row">
          <div class="col-lg-12">
            <h4>عضویت</h4>
          </div>
            <div class="col-lg-6 col-sm-12">
              <fieldset>
                <input placeholder="Your Name*"  asp-for="FirstName">
              </fieldset>
            </div>
            <div class="col-lg-6 col-sm-12">
              <fieldset>
                <input  placeholder="Your LastName*"  asp-for="LastName">
              </fieldset>
            </div>
              <div class="col-lg-6 col-sm-12">
                <fieldset>
                  <input placeholder="Your password*"  asp-for="Password">
                </fieldset>
              </div>
                <div class="col-lg-6 col-sm-12">
                  <fieldset>
                    <input  placeholder="Your Email Address"  asp-for="Email">
                  </fieldset>
                </div>
                  <div class="col-lg-6 col-sm-12">
                    <fieldset>
                      <input placeholder="Phone Number*"  asp-for="MobileNumber">
                    </fieldset>
                    </div>
                  </div>     
                    <div class="col-lg-12">
                      <fieldset>
                        <button type="submit"  class="main-button-icon">عضویت</button>
                      </fieldset>
                    </div>
                  </form>
                </div>
              </div>